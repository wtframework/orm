{
  "name": "wtframework/orm",
  "description": "What the Framework?! ORM",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "wtframework/dbal": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "WTFramework\\ORM\\": "src/"
    }
  },
  "require-dev": {
    "pestphp/pest": "^2.30"
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/src"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}